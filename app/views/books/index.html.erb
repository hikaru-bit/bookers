<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.body %></td>
      <td><%= link_to "show", book_path %></td>
      <td><%= link_to "edit", edit_book_path(@book.id) %></td>
      <td><%= link_to "destroy" destroy_book_path(@book.id), method: delete %></td>
    </tr>
  </tbody>
</table>

<h1></h1>